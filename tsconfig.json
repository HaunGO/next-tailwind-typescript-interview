{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Thing/Thing.jsx",
    "components/Thing/MicroThing.jsx",
    "components/Form/Row.jsx",
    "components/Form/Form1.jsx",
    "pages/_document.jsx",
    "pages/_app.jsx",
    "components/Brandon/Brandon.jsx",
    "app/Layout.jsx",
    "app/head.jsx",
    ".next/types/**/*.ts"
, "components/ArrayMethods.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
